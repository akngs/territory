{
  "threshold": 0,
  "reporters": ["html", "console", "badge"],
  "ignore": [
    "node_modules/**",
    "dist/**",
    "gamedata/**",
    "**/*.test.ts",
    "**/*.json",
    ".jscpd/**"
  ],
  "absolute": true,
  "gitignore": true,
  "format": ["typescript", "javascript"],
  "minLines": 3,
  "minTokens": 35,
  "maxLines": 500,
  "maxSize": "100kb",
  "output": ".jscpd",
  "blame": false,
  "silent": false,
  "verbose": false
}
